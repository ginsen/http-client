{
    "name": "ginsen/http-client",
    "description": "Http client handler with multiple asynchronous requests in parallel",
    "keywords": ["PSR", "Http", "Client", "RequestInterface", "Request", "ResponseInterface", "Response"],
    "homepage": "https://github.com/ginsen/http-client",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "ginsen",
            "email": "jgines@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-curl": "*",
        "guzzlehttp/guzzle": "^6.0 | ^7.0",
        "php-http/guzzle7-adapter": "^1.1",
        "psr/http-message": "^2.0"
    },
    "require-dev": {
        "nyholm/psr7": "^1.8",
        "phpunit/phpunit": "^12.2",
        "symfony/var-dumper": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "Ginsen\\HttpClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ginsen\\HttpClient\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin",
        "process-timeout": 0,
        "sort-packages": true
    }
}
